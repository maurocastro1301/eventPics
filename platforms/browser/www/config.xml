<?xml version='1.0' encoding='utf-8'?>
<widget id="ar.com.eventPics" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>EventPics</name>
    <description>
        EventPics.
    </description>
    <author email="" href="">
        Mauro Castro
    </author>
    <content src="index.html" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <gap:plugin name="cordova-plugin-camera" source="npm" />
    <gap:plugin name="cordova-plugin-statusbar" source="npm" />
    <access subdomains="false" uri="*" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <access origin="*" />
</widget>
